datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id     Int    @id @default(autoincrement())
  name     String
  email    String   @unique
  password String
 Reservations Reservation[]
}

model  Reservation {
  id     Int    @id @default(autoincrement())
  userId   Int
  user      User     @relation(fields: [userId], references: [id])
  start_time DateTime
  end_time   DateTime
}